@article{armstrong_simple_2019,
	title = {{SIMPLE} {AND} {HONEST} {CONFIDENCE} {INTERVALS}},
	abstract = {We consider the problem of constructing honest conﬁdence intervals (CIs) for a scalar parameter of interest, such as the regression discontinuity parameter, in nonparametric regression based on kernel or local polynomial estimators. To ensure that our CIs are honest, we derive novel critical values that take into account the possible bias of the estimator upon which the CIs are based. We show that this approach leads to CIs that are more eﬃcient than conventional CIs that achieve coverage by undersmoothing or subtracting an estimate of the bias. We give sharp eﬃciency bounds of using diﬀerent kernels, and derive the optimal bandwidth for constructing honest CIs. We show that using the bandwidth that minimizes the maximum mean-squared error results in CIs that are nearly eﬃcient and that in this case, the critical value depends only on the rate of convergence. For the common case in which the rate of convergence is n−2/5, the appropriate critical value for 95\% CIs is 2.18, rather than the usual 1.96 critical value. We illustrate our results in a Monte Carlo analysis and an empirical application.},
	language = {en},
	journal = {Working Paper},
	author = {Armstrong, Timothy B and Kolesár, Michal},
	year = {2019},
	file = {Armstrong et Kolesár - SIMPLE AND HONEST CONFIDENCE INTERVALS.pdf:C\:\\Users\\Hippo\\Zotero\\storage\\GGZGW7ZV\\Armstrong et Kolesár - SIMPLE AND HONEST CONFIDENCE INTERVALS.pdf:application/pdf}
